<div id="cuwire-{{dialogId}}" class="modal">
	<div class="modal-header">
		<h1 class="dialog-title">{{header}}</h1>
	</div>
	<div class="modal-body">{{body}}</div>
	<div class="modal-footer">
		{{footer}}
		<button data-button-id="close" class="dialog-button btn btn-80">Close</button>
	</div>
</div>
